  26 |         const form = document.querySelector("#shipping-first_name");
  27 |         return form && form.offsetParent !== null; // Checks computed visibility
  28 |       },
  29 |       { timeout: 3000 }
  30 |     );
  31 |   }
  32 |
  33 |   async fillShippingDetails(details) {
