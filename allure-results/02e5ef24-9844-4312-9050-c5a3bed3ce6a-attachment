# Test source

```ts
   1 | import { test, expect } from "@playwright/test";
   2 | import LoginPage from "../page-objects/LoginPage.js";
   3 | import ShopPage from "../page-objects/ShopPage.js";
   4 | import CartPage from "../page-objects/CartPage.js";
   5 | import CheckoutPage from "../page-objects/CheckoutPage.js";
   6 | import config from "../config/site-config.json" assert { type: "json" };
   7 |
   8 | test.describe("E2E Checkout Flow", () => {
   9 |   test("Complete checkout as logged-in user", async ({ page }) => {
  10 |     // 1. Login
  11 |     const loginPage = new LoginPage(page);
  12 |     await loginPage.navigateToLogin();
  13 |     await loginPage.login(
  14 |       config.users.valid.username,
  15 |       config.users.valid.password
  16 |     );
  17 |     await expect(page).toHaveURL(/my-account/); // Verify login success
  18 |
  19 |     //page.pause();
  20 |
  21 |     // 2. Add Product to Cart
  22 |     const shopPage = new ShopPage(page);
  23 |     await page.goto("/product/abominable-hoodie"); // Specific product page
  24 |     await shopPage.selectProductOptions("S", "Green"); // Choose color/size
  25 |     await shopPage.addToCart();
  26 |
  27 |     // V// After adding to cart:
  28 |     await shopPage.viewCart(); // New step: click "View Cart"
  29 |     // verify cart update
  30 |
  31 |     await page.waitForSelector(config.selectors.cart.cartTotal);
  32 |     const cartTotal = await page
  33 |       .locator(config.selectors.cart.cartTotal)
  34 |       .textContent();
> 35 |     expect(cartTotal).toContain("69,00");
     |                       ^ Error: expect(received).toContain(expected) // indexOf
  36 |
  37 |     // 3. Proceed to Checkout
  38 |     const cartPage = new CartPage(page);
  39 |     await cartPage.proceedToCheckout();
  40 |
  41 |     // 4. Complete Checkout
  42 |     const checkoutPage = new CheckoutPage(page);
  43 |     await checkoutPage.fillShippingDetails({
  44 |       firstName: "John",
  45 |       lastName: "Doe",
  46 |       address: "123 Main St",
  47 |       city: "Vilnius",
  48 |       postcode: "01234",
  49 |     });
  50 |     await checkoutPage.placeOrder();
  51 |
  52 |     // Final Assertion
  53 |     await expect(page).toHaveURL(/order-received/);
  54 |     await expect(page.locator(".order-success")).toBeVisible();
  55 |   });
  56 | });
  57 |
