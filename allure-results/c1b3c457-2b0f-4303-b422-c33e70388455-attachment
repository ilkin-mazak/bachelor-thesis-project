# Test source

```ts
   1 | import config from "../config/site-config.json" assert { type: "json" };
   2 |
   3 | export default class CheckoutPage {
   4 |   constructor(page) {
   5 |     this.page = page;
   6 |     this.selectors = config.selectors.checkout;
   7 |   }
   8 |
   9 |   async safeClickEditAddress() {
  10 |     const editButton = this.page.getByRole(
  11 |       this.selectors.editShippingAddress.role,
  12 |       { name: this.selectors.editShippingAddress.name }
  13 |     );
  14 |
  15 |     if (await editButton.isVisible()) {
  16 |       await editButton.click();
  17 |       // Wait for click to register but no network requirement
  18 |       await this.page.waitForTimeout(200);
  19 |     }
  20 |   }
  21 |
  22 |   async ensureBillingFormVisible() {
  23 |     // Wait for form container animation/transition
  24 |     await this.page.waitForFunction(
  25 |       () => {
  26 |         const form = document.querySelector("#shipping-first_name");
  27 |         return form && form.offsetParent !== null; // Checks computed visibility
  28 |       },
  29 |       { timeout: 3000 }
  30 |     );
  31 |   }
  32 |
  33 |   async fillShippingDetails(details) {
  34 |     await this.ensureBillingFormVisible();
  35 |
  36 |     await this.page.locator(this.selectors.firstName).fill(details.firstName);
  37 |     await this.page.locator(this.selectors.lastName).fill(details.lastName);
  38 |     await this.page.locator(this.selectors.address).fill(details.address);
  39 |     await this.page.locator(this.selectors.city).fill(details.city);
  40 |     await this.page.locator(this.selectors.postcode).fill(details.postcode);
  41 |   }
  42 |
  43 |   async placeOrder() {
  44 |     await this.page.locator(this.selectors.placeOrderButton).click();
> 45 |     await this.page.waitForURL(/order-received/, { timeout: 15000 });
     |                     ^ Error: page.waitForURL: Test timeout of 10000ms exceeded.
  46 |   }
  47 | }
  48 |
